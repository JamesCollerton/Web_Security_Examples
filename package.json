{
  "name": "web_security_example",
  "version": "1.0.0",
  "description": "A small example application used to demonstrate various security vulnerabilities",
  "main": "server.js",
  "scripts": {
    "test": "jest",
    "coverage": "jest --coverage",
    "lint": "./node_modules/.bin/eslint src/js/*",
    "codecov": "./node_modules/.bin/codecov",
    "buildDev": "rm -rf dist && webpack --mode development --config webpack.server.config.js && webpack --mode development --config webpack.dev.config.js",
    "buildProd": "rm -rf dist && webpack --mode production --config webpack.server.config.js && webpack --mode production --config webpack.prod.config.js",
    "database": "docker-compose -f ./dev-infrastructure/docker-compose.yml up",
    "start": "node ./dist/server.js"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "setupFilesAfterEnv": [
      "./src/test/setup.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JamesCollerton/React_Drag_and_Drop.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JamesCollerton/React_Drag_and_Drop/issues"
  },
  "homepage": "https://github.com/JamesCollerton/React_Drag_and_Drop#readme",
  "dependencies": {
    "@babel/runtime": "^7.6.2",
    "aws-sdk": "^2.591.0",
    "axios": "^0.19.2",
    "bootstrap": "^4.3.1",
    "config": "^3.3.1",
    "express": "^4.17.1",
    "fs": "0.0.1-security",
    "jquery": "^3.4.1",
    "popper.js": "^1.16.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "codecov": "^3.7.1",
    "css-loader": "^3.2.0",
    "dotenv": "^8.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.4.0",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-standard": "^4.0.1",
    "file-loader": "^4.2.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "mini-css-extract-plugin": "^0.8.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "style-loader": "^1.0.0",
    "terser-webpack-plugin": "^2.2.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^2.1.0",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-middleware": "^3.7.1",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-node-externals": "^1.7.2"
  }
}
